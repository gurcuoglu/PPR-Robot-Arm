<?xml version="1.0"?><robot name="planar2">	<material name="mor">		<color rgba="1 0 1 0.8" />	</material>	<material name="sari">		<color rgba="1 1 0 0.8" />	</material>	<material name="turkuaz">		<color rgba="0 1 1 0.1" />	</material>	<link name="base_link" />	<link name="link0">		<visual>			<geometry>				<mesh filename="package://base.stl" />			</geometry>			<origin xyz="0 0 0" />			<material name="turkuaz" />		</visual>		<collision>			<geometry>				<cylinder radius = "0.1" length="1" />			</geometry>			<material name="turkuaz" />		</collision>	</link>	<link name="link1">		<visual>			<geometry>				<mesh filename="package://first.stl" />			</geometry>			<origin xyz="0 0 1.75" />			<material name="mor" />		</visual>		<collision>			<geometry>				<cylinder radius = "0.1" length="1" />			</geometry>			<material name="mor" />		</collision>	</link>	<link name="link2">		<visual>			<geometry>				<mesh filename="package://second.stl" />			</geometry>			<origin xyz="0.5 0 3" />			<material name="sari" />		</visual>	</link>	<link name="end">		<visual>			<geometry>				<mesh filename="package://last.stl" />			</geometry>			<origin xyz="1.75 0 3" />		</visual>		<collision>			<geometry>				<cylinder radius = "0.1" length="1" />			</geometry>			<material name="sari" />		</collision>	</link>	<joint name="q0" type="fixed">		<parent link="base_link" />		<child link="link0" />		<origin xyz="0 0 0" />		<axis xyz="0 0 0" />	</joint>	<joint name="q1" type="prismatic">		<parent link="link0" />		<child link="link1" />		<limit effort="30" velocity="1.0" lower="-2.2" upper="1" />		<origin xyz="0 0 0" />		<axis xyz="0 0 1" />	</joint>	<joint name="q2" type="prismatic">		<parent link="link1" />		<child link="link2" />		<limit effort="30" velocity="1.0" lower="-2.2" upper="1" />		<origin xyz="0 0 0" />		<axis xyz="1 0 0" />	</joint>	<joint name="q3" type="continuous">		<parent link="link2" />		<child link="link3" />		<origin xyz="0 0 0" />		<axis xyz="0 0 1" />	</joint></robot>